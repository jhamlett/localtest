{
  "name": "jekyll-cloudcannon-template",
  "version": "1.0.0",
  "description": "Cloudcannon Suite Template is a starting point for sites hosted on Cloudcannon and built using Jekyll.",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "gulp dev",
    "dev": "concurrently --kill-others 'npm:watch-scripts' 'npm:watch-css' npm:start",
    "watch-scripts": "gulp scripts",
    "watch-css": "postcss src/_sass/main.scss --output src/css/main.css --watch",
    "build": "npm run build-css && npm run build-scripts && gulp dev:build",
    "build-css": "NODE_ENV=production postcss src/_sass/main.scss --output css/main.css",
    "build-scripts": "NODE_ENV=production gulp scripts",
    "proof": "gulp dev:build && gulp proof",
    "state": "gulp dev:build && gulp state",
    "clean": "rimraf dist src/.jekyll-cache"
  },
  "author": "Ocupop",
  "license": "MIT",
  "dependencies": {
    "gulp-cli": "^2.3.0",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remixicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@cloudcannon/suite": "2.0.3-rc1",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cssnano": "^5.0.8",
    "gulp": "^4.0.2",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.16",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-stream": "^7.0.0"
  }
}
